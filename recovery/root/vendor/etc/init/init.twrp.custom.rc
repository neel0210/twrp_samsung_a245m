on init
    start load-touch-modules

on boot
    start dmesg-logger

service dmesg-logger /sbin/sh -c "/system/bin/timeout 60 /system/bin/dmesg -w > /tmp/dmesg_a24.txt"
    class core
    oneshot
    seclabel u:r:recovery:s0
    disabled

service load-touch-modules /system/bin/modprobe --all=/vendor/lib/modules/touch/modules.load --dirname=/vendor/lib/modules/touch
    class core
    oneshot
    seclabel u:r:recovery:s0
    disabled
