#<src>                              <mnt_point>       <type>        <mnt_flags and options>      <fs_mgr_flags>
# Logical f2fs partitions
system                              /system           f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount
systen_dlkm                         /system_dlkm      f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount
system_ext                          /system_ext       f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount
vendor                              /vendor           f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount
vendor_dlkm                         /vendor_dlkm      f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount
product                             /product          f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount
odm                                 /odm              f2fs          ro,barrier=1,discard         wait,logical,first_stage_mount

# Logical ext4 partitions
system                              /system           ext4          ro,barrier=1,discard         wait,logical,first_stage_mount
systen_dlkm                         /system_dlkm      ext4          ro,barrier=1,discard         wait,logical,first_stage_mount
system_ext                          /system_ext       ext4          ro,barrier=1,discard         wait,logical,first_stage_mount
vendor                              /vendor           ext4          ro,barrier=1,discard         wait,logical,first_stage_mount
vendor_dlkm                         /vendor_dlkm      ext4          ro,barrier=1,discard         wait,logical,first_stage_mount
product                             /product          ext4          ro,barrier=1,discard         wait,logical,first_stage_mount
odm                                 /odm              ext4          ro,barrier=1,discard         wait,logical,first_stage_mount

# Logical erofs partitions
system                              /system           erofs         ro,barrier=1,discard         wait,logical,first_stage_mount
systen_dlkm                         /system_dlkm      erofs         ro,barrier=1,discard         wait,logical,first_stage_mount
system_ext                          /system_ext       erofs         ro,barrier=1,discard         wait,logical,first_stage_mount
vendor                              /vendor           erofs         ro,barrier=1,discard         wait,logical,first_stage_mount
vendor_dlkm                         /vendor_dlkm      erofs         ro,barrier=1,discard         wait,logical,first_stage_mount
product                             /product          erofs         ro,barrier=1,discard         wait,logical,first_stage_mount
odm                                 /odm              erofs         ro,barrier=1,discard         wait,logical,first_stage_mount

# Physical partitions
/dev/block/by-name/metadata         /metadata         ext4          noatime,nosuid,nodev,noauto_da_alloc,discard,journal_checksum,data=ordered,errors=panic,sync	wait,first_stage_mount,formattable,check
/dev/block/by-name/userdata         /data             f2fs          noatime,nosuid,nodev,discard,usrquota,grpquota,fsync_mode=nobarrier,reserve_root=32768,resgid=5678	wait,length=-16384,fscompress

# Add misc for GOTA
/dev/block/by-name/misc             /misc             emmc          defaults                     recoveryonly
